                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                                        <swrl:classPredicate rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#ActingCapability"/>
                                                        <swrl:argument1 rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#c"/>
