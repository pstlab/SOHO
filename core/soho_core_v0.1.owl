        <rdfs:domain rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#Robot"/>
        <rdfs:range rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#RobotInterface"/>
