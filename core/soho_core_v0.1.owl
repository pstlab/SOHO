                                <swrl:argument1 rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#i"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#hasRobotPart"/>
                                        <swrl:argument1 rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#r"/>
                                        <swrl:argument2 rdf:resource="http://pst.istc.cnr.it/ontologies/2019/01/soho#i"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
