                    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Sensor"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://pst.istc.cnr.it/ontologies/2019/01/soho#requires -->

    <owl:ObjectProperty rdf:about="http://pst.istc.cnr.it/ontologies/2019/01/soho#requires"/>
    


